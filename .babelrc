{
  "presets": [
    ["env", {
      "modules": false
    }],
    "stage-2",
    "es2015"
  ],
  "plugins": ["transform-runtime","transform-object-rest-spread",["component", [
    {
      "libraryName": "element-ui",
      "styleLibraryName": "theme-chalk"
    }
  ]]],
  "comments": false,
  "env": {
    "test": {
      "presets": ["env", "stage-2", "es2015"],
      "plugins": ["transform-es2015-modules-commonjs", "dynamic-import-node", "istanbul"]
    }
  }
}
